{
  "name": "@adonisjs/bodyparser",
  "version": "3.2.4",
  "description": "AdonisJs body parser to read and parse HTTP request bodies",
  "main": "build/providers/BodyParserProvider.js",
  "files": [
    "build/src",
    "build/adonis-typings",
    "build/providers",
    "build/config"
  ],
  "types": "./build/adonis-typings/index.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && copyfiles \"config/**/*.txt\" build",
    "lint": "eslint . --ext=.ts",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/encryption": "^1.0.4",
    "@adonisjs/fold": "^6.3.3",
    "@adonisjs/http-server": "^1.7.2",
    "@adonisjs/logger": "^1.1.8",
    "@adonisjs/mrm-preset": "^2.2.4",
    "@adonisjs/profiler": "^2.0.0",
    "@poppinss/dev-utils": "^1.0.4",
    "@types/bytes": "^3.1.0",
    "@types/end-of-stream": "^1.4.0",
    "@types/fs-extra": "^8.0.1",
    "@types/media-typer": "^0.3.0",
    "@types/multiparty": "0.0.32",
    "@types/node": "^13.7.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.6",
    "husky": "^4.2.3",
    "japa": "^3.0.1",
    "mrm": "^2.0.4",
    "np": "^5.2.1",
    "proxy-addr": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@adonisjs/http-server": "^1.0.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/utils": "^2.1.2",
    "bytes": "^3.1.0",
    "co-body": "git+https://github.com/thetutlage/co-body.git",
    "end-of-stream": "^1.4.4",
    "file-type": "^14.1.2",
    "fs-extra": "^8.1.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "media-typer": "^1.1.0",
    "multiparty": "^4.2.1",
    "uuid": "^3.4.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "adonisjs": {
    "templates": {
      "basePath": "./build/config",
      "config": [
        "bodyparser.txt"
      ]
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-bodyparser.git"
  },
  "keywords": [
    "adonisjs",
    "bodyparser",
    "multipart"
  ],
  "author": "virk,adonisjs",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-bodyparser/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-bodyparser#readme"
}
